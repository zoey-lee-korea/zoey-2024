<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.zoey.mapper.ZoeyMapper">

    <select id="findStoryById" parameterType="map" resultType="map">
    SELECT *
    FROM nerpdash_story
    WHERE story_key = #{story_id}
    </select>

</mapper>